#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/rich/Development/SVELTE/KIT/repros/gh-8516/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/rich/Development/SVELTE/KIT/repros/gh-8516/node_modules/.pnpm/node_modules"
fi
"$basedir/../../bin/esbuild"   "$@"
exit $?
